deploy_wmcore_deps()
{
   true
}

deploy_wmcore_prep()
{
  mkproj wmcore 
}

deploy_wmcore_sw()
{

  deploy_pkg -d wmcore -l app  comp cms+wmcore+WMCORE_0_7_0
  deploy_pkg -d wmcore -l py2-mysqldb comp external+py2-mysqldb+1.2.3c1-cmp6
}

deploy_wmcore_post()
{
   true
}
