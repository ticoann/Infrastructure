# vim: set ft=sh sw=2 ts=8 et :
deploy_wmcore_deps()
{
   true
}

deploy_wmcore_prep()
{
  mkproj wmcore
}

deploy_wmcore_sw()
{
  deploy_pkg -d wmcore -l app -c WMCore comp cms+wmcore+WMCORE_0_6_9
  deploy_pkg -d wmcore -l python2.6   comp external+python+2.6.4-cmp7
  deploy_pkg -d wmcore -l py2-mysqldb comp external+py2-mysqldb+1.2.3c1-cmp6
}

deploy_wmcore_post()
{
   true
}
