# vim: set ft=sh sw=2 ts=8 et :
deploy_mysql_deps()
{
   true
}

deploy_mysql_prep()
{
  mkproj mysql database logs stagingarea config
  setgroup ug+rw,o-w _couchdb $PWD/{database,logs,stagingarea}
}

deploy_mysql_sw()
{
  deploy_pkg -d mysql -l app  comp external+mysql+5.1.35-cmp

}

deploy_mysql_post()
{
  true
}
